{
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "icons": {
        "16": "waypost_16.png",
        "48": "waypost_48.png",
        "128": "waypost_128.png"
    },
    "web_accessible_resources": ["/blocked.html", "/blocked.js", "/blocked.css", "/waypost.jpg"],
    "manifest_version": 2,
    "permissions": ["history", "cookies", "activeTab", "webRequestBlocking", "webRequest", "*://*/*"],
    "author": "Ross Jacobs",
    "name": "Waypost",
    "description": "Confirm that you want to go to a domain you haven't been to before (checks history and cookies)",
    "version": "0.0.1"
  }